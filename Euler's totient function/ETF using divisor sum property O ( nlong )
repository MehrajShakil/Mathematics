#include <bits/stdc++.h>
using namespace std;

const int N = 1000001;
int phi[N +1];

void ETF ( ){
	
	for ( int i = 1 ; i<=N ; ++i ) phi[i] = i;
	
	 for ( int d = 1 ; d<=N ; ++d ){
	 	for ( int n = d*2 ; n<=N ; n+=d ){
	 		phi[n]-=phi[d];
	 	}
	 }
	 
}

int main ( ){
	
     ETF();
     
     int t , n;
     cin >> t;
     while ( t-- ){
      	   cin >> n;
      	   cout << phi[ n ] <<  "\n";
     }
	
}

/* 

divisor sum property says ,
if d(n) are d1 , d2 , d3 ......dk than
phi(d1 ) + phi(d2) + phi(d3) + .........+ phi(dk) = n.

*/
